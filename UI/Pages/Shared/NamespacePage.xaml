<Page x:Class="UI.Pages.Shared.NamespacePage"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:UI.Pages.Shared"
      xmlns:sys="clr-namespace:System;assembly=mscorlib" 
      mc:Ignorable="d" 
      Title="Wzorce Projektowe" Height="200" Width="500"
        MinHeight="200" MinWidth="500" Background="White">

    <StackPanel HorizontalAlignment="Center" VerticalAlignment="Center" Margin="0,0,0,40"> 
        <Label Content="Podaj przestrzeń nazw tworzonego kodu:" HorizontalAlignment="Center" Margin="0,0,0,0"/>
        <TextBox x:Name="txtNamespaceName" Width="200" Margin="20"/>
        <StackPanel Orientation="Horizontal" HorizontalAlignment="Center">
            <Button x:Name="btnNext" Click="btnNext_Click" Content="Dalej" Width="100" Margin="0,0,10,0"/>
            <Button x:Name="btnBack" Click="btnBack_Click" Content="Wstecz" Width="100" />
        </StackPanel>
    </StackPanel>
</Page>
